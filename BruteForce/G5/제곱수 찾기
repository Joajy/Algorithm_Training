#include<iostream>
#include<cmath>
#define fastio cin.tie(0), cout.tie(0), ios_base::sync_with_stdio(0);

using namespace std;

int n, m, answer = -1;
int area[10][10];
int dx[]{ 0, 1, 0, -1, 1, 1, -1, -1 };
int dy[]{ 1, 0, -1, 0, 1, -1, 1, -1 };

void input() {
	cin >> n >> m;
	string s;
	for (int i = 0; i < n; ++i) {
		cin >> s;
		for (int j = 0; j < m; ++j) {
			area[i][j] = s[j] - '0';
		}
	}
}

bool outRange(int x, int y) {
	return x < 0 || x >= n || y < 0 || y >= m;
}

void search(int x, int y, int tx, int ty, int value) {
	// 완전 제곱수임이 입증되면 최대값 비교
	int sq = sqrt(value);
	if (sq * sq == value) {
		answer = max(answer, value);
	}
	int nx = x + tx;
	int ny = y + ty;
	if (outRange(nx, ny)) return;
	search(nx, ny, tx, ty, value * 10 + area[nx][ny]);
}

void solution() {
	for (int i = 0; i < n; ++i) {
		for (int j = 0; j < m; ++j) {
			for (int x = -n; x < n; ++x) {
				for (int y = -m; y < m; ++y) {
					if (x == 0 && y == 0) continue;
					search(i, j, x, y, area[i][j]);
				}
			}
		}
	}
	cout << answer;
}

int main() {
	fastio;
	input();
	solution();
	return 0;
}
