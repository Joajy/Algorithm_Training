class Solution {
    public int jump(int[] nums) {
        int n = nums.length;
        int[] dp = new int[n];
        for(int i = 0; i < n; ++i) {
            dp[i] = (int)1e9;
        }
        dp[0] = 0;
        for(int i = 0; i < n; ++i) {
            for(int j = 0; j <= nums[i]; ++j) {
                if(i + j == n) break;
                dp[i + j] = Math.min(dp[i + j], dp[i] + 1);
            }
        }
        return dp[n - 1];
    }
}
