/* 직전의 값을 가져온다 */
class Solution {
    public int maximumLength(int[] nums, int k) {
        int n = nums.length, answer = 0;
        int[][] dp = new int[n][k];
        for(int i = 0; i < n; ++i) {
            for(int j = 0; j < i; ++j) {
                int sum = (nums[i] + nums[j]) % k;
                dp[i][sum] = dp[j][sum] + 1;
                answer = Math.max(answer, dp[i][sum]);
            }
        }
        return answer + 1;
    }
}

/* num값, 직전값 + 1 -> 직전값, num값 */
class Solution {
    public int maximumLength(int[] nums, int k) {
        int n = nums.length, answer = 0;
        int[][] dp = new int[k][k];
        for (int num : nums) {
            num %= k;
            for (int prev = 0; prev < k; prev++) {
                dp[prev][num] = dp[num][prev] + 1;
                answer = Math.max(answer, dp[prev][num]);
            }
        }
        return answer;
    }
}
